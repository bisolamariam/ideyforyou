{
  "expo": {
    "name": "idey",
    "slug": "idey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ideyforyouLogo.jpg",
    "scheme": "ideyforyouapp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ideyforyouLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ideyforyou.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ideyforyouLogo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ideyforyou-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2a27cfd3-c0aa-41c3-994e-19016c2c2cc4"
      }
    },
    "owner": "ishaaaq"
  }
}
